{
    "name": "josezenem/laravel-slugidable",
    "description": "A package for Laravel that creates slugs for Eloquent models based on title and ID",
    "keywords": [
        "josezenem",
        "laravel",
        "laravel-slugidable",
        "slugidable",
        "sluggable",
        "models",
        "php"
    ],
    "homepage": "https://github.com/josezenem/laravel-slugidable",
    "license": "MIT",
    "authors": [
        {
            "name": "Jose Jimenez",
            "email": "jose@jimenez.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/database": "^8.0|^9.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "6.23|^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Josezenem\\Slugidable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Josezenem\\Slugidable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
